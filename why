*2. attibutes 
  Project.findOne({
    where: {title: 'aProject'},
    attributes: ['id', ['name', 'title']]
  }).then(function(project) {
    // project will be the first entry of the Projects table with the title 'aProject' || null
    // project.title will contain the name of the project
  })
  
